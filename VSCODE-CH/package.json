{
  "name": "ch-syntax-highlighting",
  "displayName": "CH Syntax Highlighting",
  "description": "Syntax highlighting for the CH language in Visual Studio Code",
  "version": "1.0.0",
  "publisher": "G. CENTER",
  "icon": "icon.png",
  "main": "./src/settings.js",

  "activationEvents": [
    "onLanguage:ch"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "ch",
        "aliases": ["CH"],
        "extensions": [".ch"]
      }
    ],
    "completionItems": [
      {
        "language": "ch",
        "path": "./completions.json"
      }
    ],
    "snippets": [
      {
        "language": "ch",
        "path": "./snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "ch",
        "scopeName": "source.ch",
        "path": "./syntaxes/ch.tmLanguage.json"
      }
    ]
  }
}
